<div class="nested-fields image-upload-field">
  <div class="input-upload-description" >
    <%= f.label "Upload Images" %>
    <span class="smaller">
      If the image includes the client or client artwork fill out Art/Media Release Form <%= link_to "here", "http://dashboard.awbw.org/resources/859" %>
    </span>

  </div>

  <div class="form-group">
    <%= f.label :image, class: "btn btn-primary btn--small" do %>
      <%= f.file_field :image, class: ''%>
        Choose File
    <% end %>
  </div>

  <div class="inline-block small">
    <%= link_to_remove_association "Remove Image", f , class:"btn-underline"%>
  </div>
</div>

